# My Next Home POC Environment File

# Database
DATABASE_URL="file:./db.sqlite"

# Integration Mode: 'mock' (default) or 'real'
INTEGRATIONS_MODE=mock

# Real Integration Keys (only needed if INTEGRATIONS_MODE=real)
# RESO/MLS Integration
# RESO_API_KEY=your_api_key_here
# RESO_API_BASE_URL=https://api.reso.org/v2

# Google Maps (for real commute time)
# GOOGLE_MAPS_API_KEY=your_key_here

# Mapbox (alternative for commute time)
# MAPBOX_API_KEY=your_key_here

# GreatSchools (for real school data)
# GREATSCHOOLS_API_KEY=your_key_here

# Feature Flags
FEATURE_REAL_GEOCODING=false
FEATURE_REAL_COMMUTE=false

# Next.js
NEXT_PUBLIC_APP_URL=http://localhost:3000
